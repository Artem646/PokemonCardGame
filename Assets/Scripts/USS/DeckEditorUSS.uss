#unity-content-container {
    flex-direction: row;
    flex-wrap: wrap;
    justify-content: space-evenly;
    align-content: stretch;
}

#unity-text-input {
    background-color: rgba(0, 0, 0, 0);
    margin: 0;
    padding: 0;
    height: 100%;
    font-size: 35px;
    -unity-text-align: middle-center;
    text-align: center;
    color: rgb(255, 255, 255);
    border-top-width: 0;
    border-right-width: 0;
    border-bottom-width: 0;
    border-left-width: 0;
}
